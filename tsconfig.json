{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "strict": true,
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es5",
        "lib": ["es5", "es6", "dom"],
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "types": [
            "jest"
        ]
    },
    "include": [
        "./src/**/*",
        "./__tests__/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}